{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "CareSyncOS",
  "version": "1.0.0",
  "identifier": "com.caresync.os",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "CareSyncOS",
        "label": "main",
        "url": "https://prospine.in/admin/",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
